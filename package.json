{
  "name": "gistom",
  "version": "1.0.0",
  "description": "GitHub gist atomic editor.",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mujahidk/gistom.git"
  },
  "scripts": {
    "start": "npm run build && electron .",
    "devtools": "npm run build && electron . --dev-tools",
    "build": "gulp clean && gulp",
    "package": "npm run build && electron-packager . Gistom --overwrite"
  },
  "keywords": [
    "GitHub",
    "Gist",
    "Editor"
  ],
  "author": "Mujahid Khaleel",
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.13.0",
    "electron": "^1.6.6",
    "electron-packager": "^8.6.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.3.0",
    "gulp-react": "^3.1.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "octonode": "^0.7.12",
    "prism-languages": "^0.3.3",
    "prismjs": "^1.6.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
