  "license": "MIT",
  "devDependencies": {
    "asar": "^0.13.0",
    "bootstrap": "^3.3.7",
    "electron": "^1.6.6",
    "electron-packager": "^8.6.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.3.0",
    "gulp-react": "^3.1.0"
  }
}
